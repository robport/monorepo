{
  "networkMode": "awsvpc",
  "family": "monorepo",
  "placementConstraints": [],
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::676439790032:role/ecsTaskExecutionRole",
  "volumes": [],
  "memory": "512",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "containerDefinitions": [
    {
      "memoryReservation": 512,
      "environment": [
        {
          "name": "PORT",
          "value": "80"
        }
      ],
      "secrets": [
        {
          "valueFrom": "arn:aws:secretsmanager:eu-west-2:676439790032:secret:prod/auth-secret-7rztLG",
          "name": "AUTH_SECRET"
        },
        {
          "valueFrom": "arn:aws:secretsmanager:eu-west-2:676439790032:secret:prod/maria-db-url-rJx5m1",
          "name": "MARIA_DB_URL"
        }, {
          "valueFrom": "\narn:aws:secretsmanager:eu-west-2:676439790032:secret:prod/mongo-db-url-VJv4yT",
          "name": "MONGO_DB_URL"
        }
      ],
      "name": "monorepo",
      "mountPoints": [],
      "image": "676439790032.dkr.ecr.eu-west-2.amazonaws.com/monorepo:889a4a824abc13ec2ff6952ac2f1bc3ae1b783f0",
      "cpu": 0,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "ecs",
          "awslogs-group": "/ecs/monorepo"
        }
      },
      "essential": true,
      "volumesFrom": []
    }
  ]
}
